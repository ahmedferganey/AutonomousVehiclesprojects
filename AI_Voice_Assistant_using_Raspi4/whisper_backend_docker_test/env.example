# Whisper ONNX Backend - Environment Configuration
# Copy this file to .env and customize as needed

# Model Configuration
MODEL_NAME=whisper-base-onnx
MODELS_DIR=/app/models

# Logging
LOG_LEVEL=INFO

# ONNX Runtime Configuration
ONNX_NUM_THREADS=4

# Audio Settings
SAMPLE_RATE=16000
MAX_AUDIO_LENGTH=30

# Cache Configuration
MAX_CACHE_SIZE_MB=500
